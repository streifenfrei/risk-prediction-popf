model: "simplenet"
workspace: "workspace"
training:
  folds: 10
  train_size: 0.8
  epochs: 100
  optimizer: "adam"
data:
  path: "data"
  blacklist: []
  input_type: resize # one of [crop, sample, resize]
  masked: false
  crop:
    type: "roi"  # one of [full, fixed, roi]
  sample:
    size: [100, 50, 15]
    count: 100
  resize:
    size: [100, 50, 15]
    use_ratio_vector: false
  batch_size: 2
  loader_threads: 8
shutdown_system: false
