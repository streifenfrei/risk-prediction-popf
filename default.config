model: "simplenet"
workspace: "workspace"
training:
  folds: 10
  epochs: 100
  optimizer: "adam"
  es_patience: 10
data:
  path: "data"
  crop: "seg"  # one of [none, fixed, roi, seg]
  sample: false
  sample_size: [100, 50, 15]
  sample_coverage: 0.8
  sample_count: 10
  batch_size: 2
  loader_threads: 8