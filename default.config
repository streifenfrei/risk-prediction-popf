model: "simplenet"
workspace: "workspace"
training:
  folds: 10
  epochs: 100
  optimizer: "adam"
data:
  path: "data"
  # blacklist: [8940, ...]
  crop: "seg"  # one of [none, fixed, roi, seg]
  sample: false
  sample_size: [100, 50, 15]
  resize_to_sample_size: false
  sample_count: 100
  batch_size: 2
  loader_threads: 8